<Window x:Class="FilesOrganizer.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FilesOrganizer.Views"
        mc:Ignorable="d"
        Title="Files Organizer" Height="500" Width="900"
        WindowState="Maximized">
    <Window.Icon>
        <BitmapImage UriSource="pack://application:,,,/FilesOrganizer;component/images/applogo.png"/>
    </Window.Icon>
    <Grid>
        <ContentControl Grid.Column="1" Content="{Binding Path=Navigation.CurrentView}">
        </ContentControl>
    </Grid>
</Window>
